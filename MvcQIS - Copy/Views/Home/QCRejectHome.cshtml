@model IEnumerable<MvcQIS.Models.Main>
@{
    ViewBag.Title = "QC Reject Home";
}

<h2>QC Reject</h2>

<p>
    @Html.ActionLink("Add QC Reject", "AddQCReject")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.entry_date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.entry_date)
        </td>
        <td>
            @Html.ActionLink("View", "ViewQCReject", new { id=item.entry_date, key2 = item.plant_id }) |
            @*@Html.ActionLink("Details", "Details", { id=item.entry_date }) |
            @Html.ActionLink("Delete", "Delete", { id=item.entry_date })*@
        </td>
    </tr>
